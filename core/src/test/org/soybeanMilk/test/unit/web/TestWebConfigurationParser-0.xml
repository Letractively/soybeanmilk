<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE soybean-milk PUBLIC "-//SoybeanMilk//DTD soybeanMilk web//EN" "http://soybeanmilk.googlecode.com/files/soybeanMilk-web-1.0.dtd">
<soybean-milk>
	<global-config>
		
		<type-target-handler>
			<target-handler handle-type="JSON" class="org.soybeanMilk.test.unit.web.TestWebConfigurationParser$JsonTargetHandler"/>
			<target-handler handle-type="forward" class="org.soybeanMilk.test.unit.web.TestWebConfigurationParser$JsonTargetHandler"/>
			
			<target-handler handle-type="pdf, mp3 , jpeg" class="org.soybeanMilk.test.unit.web.TestWebConfigurationParser$JsonTargetHandler"/>
		</type-target-handler>
		
	</global-config>
	
	<executables>
	
		<action name="exe0">
			<target />
		</action>
		
		<action name="exe1">
			<target url="url" type="forward" />
		</action>
		
		<action name="exe2">
			<target type="json" />
		</action>
		
		<action name="exe3">
			<target type="JSON" />
		</action>
		
	</executables>
	
</soybean-milk>
