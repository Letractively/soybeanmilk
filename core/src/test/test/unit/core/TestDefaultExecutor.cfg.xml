<?xml version="1.0" encoding="UTF-8"?>
<soybean-milk>
	
	<global-config>
		
		<interceptor before="before" after="after" exception="exception" execution-key="execution" />
		
	</global-config>
	
	
	<resolvers>
		<resolver id="resolver" class="test.unit.core.TestDefaultExecutor$ResolverForTest" />
	</resolvers>
	
	<executables>
		
		<invoke name="before">
			before=resolver.before(execution);
		</invoke>
		
		<invoke name="after">
			after=resolver.after(execution);
		</invoke>
		
		<invoke name="exception">
			exception=resolver.exception(execution);
		</invoke>
		
		<action name="hello">
			<invoke>
				resolver.hello();
			</invoke>
		</action>
		
		<action name="helloThrow">
			<invoke>
				resolver.helloThrow();
			</invoke>
		</action>
		
	</executables>
	
</soybean-milk>