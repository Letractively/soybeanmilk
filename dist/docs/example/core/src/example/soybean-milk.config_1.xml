<?xml version="1.0" encoding="UTF-8"?>
<soybean-milk>

	<resolvers>
		<resolver id="helloResolver" class="example.resolver.HelloResolver" />
	</resolvers>
	
	<executables>
		
		<action name="helloAction">
			<invoke>helloResult=helloResolver.hello(param.helloTo, param.helloRepeat)</invoke>
			<invoke>helloResolver.printObject(helloResult)</invoke>
			<invoke>commonResolver.handleTarget(request,response)</invoke>
		</action>
		
	</executables>
	
</soybean-milk>