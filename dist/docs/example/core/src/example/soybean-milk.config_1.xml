<?xml version="1.0" encoding="UTF-8"?>
<soybean-milk>
	
	<resolvers>
		<resolver id="helloResolver" class="example.resolver.HelloResolver" />
	</resolvers>
	
	<executables>
		
		<action name="helloAction">
			<invoke>
				 request.helloResult = helloResolver.hello(
				   helloTo ,
				   10)
			</invoke>
			<invoke>
				helloResolver.printObject(
					request.helloResult)
			</invoke>
			<invoke>
				helloResolver.printObject(true)
			</invoke>
			<invoke>
				helloResolver.printObject(false)
			</invoke>
			<invoke>
				helloResolver.printObject("I' am\t a \"string")
			</invoke>
			<invoke>
				helloResolver.printObject('c')
			</invoke>
			<invoke>
				helloResolver.printObject('\'')
			</invoke>
			<invoke>
				helloResolver.printObject('\\')
			</invoke>
			<invoke>
				helloResolver.printObject('\t')
			</invoke>
			<invoke>
				helloResolver.printObject('\f')
			</invoke>
			<invoke>
				helloResolver.printObject("''\"a\tb\ncd\ref")
			</invoke>
		</action>
		
	</executables>
	
</soybean-milk>