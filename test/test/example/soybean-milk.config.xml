<?xml version="1.0" encoding="UTF-8"?>
<soybean-milk>
	<resolvers>
		<resolver id="testResolver" class="test.example.resolver.TestResolver" />
	</resolvers>
	
	<executables>
		<invoke name="hello" resolver="testResolver">
			<arg key="user" />
		</invoke>
		
		<action name="multiply">
			<invoke method="multiply" resolver="testResolver" result-key="multiplyResult">
				<arg key="mul" />
			</invoke>
		</action>
		
		<action name="testAction">
			<invoke method="getDate" resolver="testResolver" result-key="date" />
			<ref name="formatDate" />
		</action>
		
		<invoke name="formatDate" resolver-class="test.example.resolver.TestResolver" result-key="formatResult">
			<arg key="date" />
			<arg value="yyyy-MM-dd HH:mm:ss" />
		</invoke>
		
	</executables>
	
</soybean-milk>